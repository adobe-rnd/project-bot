{
  "name": "org-project-bot",
  "version": "1.0.0",
  "description": "Org Project Bot",
  "author": "",
  "license": "Apache-2.0",
  "repository": "git@github.com:adobe-rnd/project-bot.git",
  "homepage": "https://github.com/adobe-rnd/project-bot",
  "bugs": "https://github.com/adobe-rnd/project-bot/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "nyc mocha && npm run lint",
    "test-ci": "npm run lint && nyc mocha --reporter mocha-junit-reporter && nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build": "wskbot",
    "deploy": "wskbot --deploy --test --no-hints"
  },
  "wsk": {
    "name": "projectbot/main@1.0.0",
    "params-file": [
      "secrets/secrets.env"
    ],
    "static": [
      "views/index.hbs"
    ],
    "github-key": "secrets/orgprojectbot.2019-02-20.private-key.pem"
  },
  "dependencies": {
    "@adobe/probot-serverless-openwhisk": "^4.0.0",
    "fs-extra": "^7.0.1",
    "js-yaml": "^3.13.1",
    "probot": "^9.3.2"
  },
  "devDependencies": {
    "@adobe/openwhisk-probot-builder": "^1.0.0",
    "ajv": "^6.10.2",
    "codecov": "^3.5.0",
    "eslint": "^6.2.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "lodash": "^4.17.15",
    "mocha": "^6.2.0",
    "mocha-junit-reporter": "^1.23.1",
    "nyc": "^14.1.1",
    "request": "^2.88.0",
    "sinon": "^7.4.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  }
}
