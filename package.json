{
  "name": "org-project-bot",
  "version": "1.0.0",
  "description": "Org Project Bot",
  "author": "",
  "license": "Apache-2.0",
  "repository": "git@github.com:adobe-rnd/project-bot.git",
  "homepage": "https://github.com/adobe-rnd/project-bot",
  "bugs": "https://github.com/adobe-rnd/project-bot/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "nyc mocha && npm run lint",
    "test-ci": "npm run lint && nyc mocha --reporter mocha-junit-reporter && nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build": "wskbot",
    "deploy": "wskbot --deploy --test --no-hints"
  },
  "wsk": {
    "name": "projectbot/main@1.0.0",
    "params-file": [
      "secrets/secrets.env"
    ],
    "static": [
      "views/index.hbs"
    ],
    "github-key": "secrets/orgprojectbot.2019-02-20.private-key.pem"
  },
  "dependencies": {
    "@adobe/probot-serverless-openwhisk": "^4.0.26",
    "fs-extra": "^7.0.1",
    "js-yaml": "^3.14.0",
    "probot": "^9.11.5"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "1.1.3",
    "@adobe/openwhisk-probot-builder": "1.0.30",
    "ajv": "6.12.2",
    "codecov": "3.7.0",
    "eslint": "7.3.1",
    "eslint-plugin-header": "3.0.0",
    "eslint-plugin-import": "2.21.2",
    "lodash": "4.17.15",
    "mocha": "6.2.3",
    "mocha-junit-reporter": "1.23.3",
    "nyc": "14.1.1",
    "request": "2.88.2",
    "sinon": "7.5.0"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  }
}
